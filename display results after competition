def giveresult(s1,s2,s3):
   if s1<s2:
       return "Good"
   elif s1>=s2 and s1<=s3:
       return "Best"
   else:
       return "Can do better"
s1=int(input("enter s1:"))
s2=int(input("enter s2:"))
s3=int(input("enter s3:"))
print(giveresult(s1,s2,s3))

output:
enter s1:10
enter s2:12
enter s3:15
Good
